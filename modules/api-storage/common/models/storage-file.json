{
  "name": "StorageFile",
  "description": "Storage: Manage Files in a Domain",
  "base": "SystemBaseModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "SystemDomain": {
      "required": false
    },
    "SystemUser": {}
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "created": {
      "type": "Date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "Date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "events": {
      "type": "hasMany",
      "model": "Event"
    },
    "pages": {
      "type": "hasMany",
      "model": "Page"
    },
    "posts": {
      "type": "hasMany",
      "model": "Post"
    },
    "products": {
      "type": "hasMany",
      "model": "Product"
    }
  },
  "acls": [],
  "methods": {}
}
